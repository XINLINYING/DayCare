package edu.neu.csye6200;

import java.util.ArrayList;
import java.util.List;

/**
 *
 * @author JiayingXie
 */
public abstract class AbstractList {
    public abstract List<Person> getPersonList();
    public abstract void generatePersonList();
    public abstract List<Person> generateAndGetList();
	public abstract void addPersonToList(Person p);
}
